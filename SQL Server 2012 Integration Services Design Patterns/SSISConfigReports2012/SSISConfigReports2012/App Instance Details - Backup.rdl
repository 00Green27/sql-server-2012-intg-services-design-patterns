  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="AppInstanceID">
          <DataField>AppInstanceID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ApplicationName">
          <DataField>ApplicationName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="StartDateTime">
          <DataField>StartDateTime</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="RunSeconds">
          <DataField>RunSeconds</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Status">
          <DataField>Status</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>SSISConfig</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>Select
   ap.AppInstanceID
  ,a.ApplicationName
  ,ap.StartDateTime
  ,DateDiff(ss,ap.StartDateTime,ap.EndDateTime) As RunSeconds
  ,ap.Status
 from log.SSISAppInstance ap
 Join cfg.Applications a
   On ap.ApplicationID = a.ApplicationID
 Where a.ApplicationName = @AppName   
 order by AppInstanceID desc</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@AppName">
            <Value>=Parameters!AppName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
  </DataSets>
  <ReportParameters>
    <ReportParameter Name="AppName">
      <DataType>String</DataType>
      <Prompt>App Name</Prompt>
    </ReportParameter>
